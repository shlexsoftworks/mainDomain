Toggle.MouseButton1Click:Connect(ToggleUI)

Exploits.MouseButton1Click:Connect(function()
	if ExploitsOpen == true and db == false then
		CloseExploits()
	elseif ExploitsOpen == false and db == false then
		OpenExploits()
	end
end)

Music.MouseButton1Click:Connect(function()
	if MusicOpen == true and db == false then
		CloseMusic()
	elseif MusicOpen == false and db == false then
		OpenMusic()
	end
end)

Info.MouseButton1Click:Connect(function()
	if InfoOpen == true and db == false then
		CloseInfo()
	elseif InfoOpen == false and db == false then
		OpenInfo()
	end
end)

Home.MouseButton1Click:Connect(function()
	if HomeOpen == true and db == false then
		CloseHome()
	elseif HomeOpen == false and db == false then
		OpenHome()
	end
end)


Copy.MouseButton1Click:Connect(function()
	if game.CreatorId ~= 0 then
	setclipboard(DiscordLink)
	Copy.Text = "Copied"
	wait(1)
	Copy.Text = "Copy to Clipboard"
	end	
end)

Play.MouseButton1Click:Connect(function()
	if tonumber(SoundId.Text) then
		if not MediaMain:FindFirstChildWhichIsA("Sound") then
			MusicSound = Instance.new("Sound",MediaMain)
			MusicSound.Volume = 0.5
			MusicSound.SoundId = "rbxassetid://"..tonumber(SoundId.Text)
			SoundId.Text = ""
			MusicSound.TimePosition = 0
			MusicSound:Play()
			Notify("Now Playing Music, tap Stop to end","GothamBold")
		elseif MediaMain:FindFirstChildWhichIsA("Sound") then
			MediaMain:FindFirstChildWhichIsA("Sound").Volume = 0.5
			MediaMain:FindFirstChildWhichIsA("Sound").SoundId = "rbxassetid://"..tonumber(SoundId.Text)
			MediaMain:FindFirstChildWhichIsA("Sound").TimePosition = 0
			MediaMain:FindFirstChildWhichIsA("Sound"):Play()
			Notify("Now Playing Music","GothamBold")
		end
	end
end)

Stop.MouseButton1Click:Connect(function()
	if not MediaMain:FindFirstChildWhichIsA("Sound") then
		Notify("Couldn't find music to pause","GothamSemibold")
	else
		MediaMain:FindFirstChildWhichIsA("Sound").Volume = 0
		Notify("Removed Music","GothamBold")
	end
end)

RemBG.MouseButton1Click:Connect(function()
	local FoundInstance = false
	for _, Object in ipairs(workspace:GetChildren()) do
		if Object.ClassName == "Sound" and string.find(string.lower(Object.Name),"music") or string.find(string.lower(Object.Name),"sound") or string.find(string.lower(Object.Name),"bg") then
			Object.Volume = 0
			FoundInstance = true
		end
	end
	if FoundInstance == false then
		Notify("Error finding music instance","GothamBold")
	else
		Notify("Found music instance, and changed volume to 0","GothamBold")
		FoundInstance = false
	end
end)

game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(item)
	if item.ClassName == "Tool" then
		if uiopened == false then
			Toggle:TweenPosition(UDim2.new(0.488, 0, 0.77, 0),"Out", "Quint", 0.4, true)
		end
	end
end)

game.Players.LocalPlayer.Backpack.ChildRemoved:Connect(function()
	local Tools = false
	for _, Child in ipairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if Child.ClassName == "Tool" then
			-- Still tools, keep current location
			Tools = true
		end
	end
	for _, Child in ipairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if Child.ClassName == "Tool" then
			Tools = true
		end
	end
	if Tools == false then
		if uiopened == false then
			Toggle:TweenPosition(UDim2.new(0.488, 0, 0.829, 0),"Out", "Quint", 0.4, true)
		end
	end
end)

Start()
LoadExploitSection()

local function getTime()
	local date = os.date("*t")
	return ("%02d:%02d"):format(((date.hour % 24) - 1) % 12 + 1, date.min)
end

local TimeLabel = Functions.Time

while wait(1) do
	if Time.Position == UDim2.new(0.029,0,1,0) then
		Time:TweenPosition(UDim2.new(0.029, 0, 0.335, 0),"Out","Quint",0.6)
	end
	local currentHour = os.date("*t")["hour"]
	TimeLabel.Text = getTime()
end

if game.CreatorId ~= 0 then
local mt = getrawmetatable(game)
local old = mt.__namecall
local protect = newcclosure or protect_function

if not protect then
	protect = function(f) return f end
end

setreadonly(mt, false)
mt.__namecall = protect(function(self, ...)
	local method = getnamecallmethod()
	if method == "Kick" then
		Notify("We stopped this game from kicking you locally","GothamBold")
		wait(9e9)
		return
	end
	return old(self, ...)
end)

hookfunction(game.Players.LocalPlayer.Kick,protect(function() wait(9e9) end))
end
