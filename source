UITextSizeConstraint_24.MaxTextSize = 18





local VersionNum = 0.432
local VersionFull = tostring(VersionNum).." | Patch 0.432"
local VFeature = "Added so tools don't cover up the toggle button"


local BannerSectionPos = 0
local ExploitsOpen = false
local MusicOpen = false
local HomeOpen = false
local DiscordOpen = false
local InfoOpen = false
local uiopened = false
local db = false
local DiscordLink = "https://discord.gg/jyySpgkFSp"
local MusicSound
local NotifStack = {}

function figureNotifs(Stack,Container)

	local stacksize = 0

	local i = #Stack
	while i > 0 do
		local gui = Stack[i]
		if gui then
			if stacksize == 0 then
				stacksize = 0.95
			else
				stacksize = stacksize - 0.075
			end
			local desiredpos = UDim2.new(0.5,0,stacksize,0)
			if gui.Position ~= desiredpos then
				gui:TweenPosition(desiredpos,"Out","Quint",0.3,true)
			end
		end
		i = i-1
	end
end

function Notify(Content,Font)
	spawn(function()
		local notificationContainer = nContainer
		local Notification = notificationContainer:WaitForChild('Template')	
		local notifClone = Notification:Clone()
		local notifContent = notifClone:WaitForChild('Content')
		if Font == "GothamSemiBold" then
			Font = "GothamSemibold"
		end
		notifContent.Font = Font
		notifContent.TextTransparency = 1
		notifContent.BackgroundTransparency = 1
		notifClone.Name = 'Notif Clone'
		notifClone.Visible = true
		notifContent.Text = Content
		notifClone.Parent = nContainer
		notifClone.Position = UDim2.new(0.5,0,1.2,0)
		wait(0.001)
		notifContent.Size = UDim2.new(0, math.max(18, notifContent.TextBounds.X * 1.2),1, 0)
		wait(0.5)
		notifClone:TweenPosition(UDim2.new(0.5,0,0.95,0),'Out','Quint',0.3,true)
		local transitionInfo = TweenInfo.new(0.35, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {TextTransparency = 0})
		tween:Play()
		local transitionInfo = TweenInfo.new(0.35, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {TextStrokeTransparency = 0.8})
		tween:Play()
		table.insert(NotifStack,notifClone)
		figureNotifs(NotifStack,notificationContainer)


		wait(7)
		for a,b in pairs(NotifStack) do
			if b == notifClone then
				table.remove(NotifStack,a)
			end
		end
		wait(0.05)
		notifClone:TweenPosition(UDim2.new(0.5,0,notifClone.Position.Y.Scale+0.1,0),'Out','Quint',0.3,true)
		local transitionInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {TextTransparency = 1})
		tween:Play()
		local transitionInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {TextStrokeTransparency = 1})
		tween:Play()
		wait(0.305)
		notifClone:Destroy()
		figureNotifs(NotifStack,notificationContainer)
	end)
end

function Start()
	Text.Text = "WELCOME, "..string.upper(game.Players.LocalPlayer.Name).." TO DOMAIN"
	game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
	for _, UiInstance in ipairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
	if UiInstance.Name == "mainDomain" and UiInstance ~= mainDomain then
		Notify("Removed previous interface","GothamBold")
		UiInstance:Destroy()
	end
	end
	Notify("Booting DOMAIN v"..tostring(VersionNum),"GothamBold")
	UpdatePanel.feat.Text = VFeature
	VersioPanel.Text.Text = "DOMAIN "..tostring(VersionNum)
	UpdatePanel.upd.Text = VersionFull 
	Exploits_2.TemplateBanner.Visible = false
	Music.Position = Home.Position
	Exploits.Position = Home.Position
	Time.Position = UDim2.new(0.029,0,1,0)
	Info.Position = UDim2.new(0.9,0,1,0)
	Functions.Position = UDim2.new(0.277, 0, 1, 0)
	local l = false
	for _, Item in ipairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		l = true
		break
	end
	for _, Item2 in ipairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if Item2.ClassName == "Tool" then
			l = true
			break
		end
	end
	if l == true then
		Toggle.Position = UDim2.new(0.488, 0, 0.77, 0)
	else
		Toggle.Position = UDim2.new(0.488, 0, 0.829, 0)
	end
	Toggle.ImageTransparency = 1
	wait(2.5)
	Functions:TweenPosition(UDim2.new(0.277, 0, 0.781, 0),"Out", "Quint", 0.7)
	Toggle:TweenPosition(UDim2.new(0.488, 0, 0.742, 0),"Out", "Quint", 0.7)
	wait(0.2)
	for _, UIIns in ipairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
		if string.find(UIIns.Name,"tool") or string.find(UIIns.Name,"hotbar") then
			UIIns.Enabled = false
		end
	end
	local transitionInfo = TweenInfo.new(0.9, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {ImageTransparency = 0})
	tween:Play()
	uiopened = true
	Music:TweenPosition(UDim2.new(0.4, 0, 0.2, 0),"Out","Quint",0.6)
	wait(0.15)
	Exploits:TweenPosition(UDim2.new(0.3, 0, 0.2, 0),"Out","Quint",0.6)
	wait(0.3)
	Info:TweenPosition(UDim2.new(0.9, 0, 0.2, 0),"Out","Quint",0.6)
	Notify("Successfully booted DOMAIN","GothamBold")
	Notify("DOMAINClient Anti-Kick active","GothamBold")
	
end

