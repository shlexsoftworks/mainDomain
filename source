		Colour = Color3.fromRGB(71, 116, 170),
		Code = "https://pastebin.com/raw/euzqMZgq"
	},
	
	
}

function LoadExploitSection()
	for _, FeaturedExploit in pairs(FeaturedExploits) do
		local banner = TemplateBanner:Clone()
		banner.Title.Text = FeaturedExploit.Name
		banner.Author.Text = FeaturedExploit.Author
		banner.Name = FeaturedExploit.Name
		if BannerSectionPos == 0 then
			BannerSectionPos = 0.025
		else
			BannerSectionPos = BannerSectionPos + 0.15
		end
		banner.Parent = TemplateBanner.Parent
		banner.Visible = true
		banner.Position = UDim2.new(BannerSectionPos, 0, 0.065, 0)
		if FeaturedExploit.ImageID ~= 0 then
			banner.CustomBG.Image = "rbxassetid://"..FeaturedExploit.ImageID
		else
			if FeaturedExploit.Colour ~= Color3.fromRGB(0,0,0) then
				banner.BackgroundColor3 = FeaturedExploit.Colour
			end
		end
	end
	for _, Exploit in ipairs(Exploits_2:GetChildren()) do
		if Exploit:FindFirstChild("Action") and Exploit.Name ~= "TemplateBanner" then
			Exploit.Action.MouseButton1Click:Connect(function()
				for _, FeaturedExploit in pairs(FeaturedExploits) do
					if FeaturedExploit.Name == Exploit.Name then
						ExecuteExploit(FeaturedExploit.Code)
						Notify("Successfully booted "..FeaturedExploit.Name,"GothamBold")
					end
				end
			end)
		end
	end
end

function ToggleUI()
	if uiopened == true then
		if ExploitsOpen == true then
			CloseExploits()
		end
		if HomeOpen == true then
			CloseHome()
		end
		if MusicOpen == true then
			CloseMusic()
		end
		if InfoOpen == true then
			CloseInfo()
		end
		for _, UIIns in ipairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
			if string.find(string.lower(UIIns.Name),"tool") or string.lower(UIIns.Name) == "toolbar" or string.lower(UIIns.Name) == "tools" then
				UIIns.Enabled = false
			end
		end
		game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
	local transitionInfo = TweenInfo.new(0.7, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {Rotation = 180})
	tween:Play()
	
	Functions:TweenPosition(UDim2.new(0.277, 0, 1, 0),"In", "Quint", 0.7)
		wait(0.37)
		local l = false
		for _, Item in ipairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			l = true
			break
		end
		for _, Item2 in ipairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if Item2.ClassName == "Tool" then
				l = true
				break
			end
		end
		if l == true then
			Toggle:TweenPosition(UDim2.new(0.488, 0, 0.77, 0),"Out", "Quint", 0.7)
		else
			Toggle:TweenPosition(UDim2.new(0.488, 0, 0.829, 0),"Out", "Quint", 0.7)
		end
		
		wait(0.71)
		local l = false
		for _, Item in ipairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			l = true
			break
		end
		for _, Item2 in ipairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if Item2.ClassName == "Tool" then
				l = true
				break
			end
		end
		if l == true then
			Toggle.Position = UDim2.new(0.488, 0, 0.77, 0)
		else
			Toggle.Position = UDim2.new(0.488, 0, 0.829, 0)
		end
	uiopened = false
	elseif uiopened == false then
		
		for _, UIIns in ipairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
			if string.find(string.lower(UIIns.Name),"tool") or string.lower(UIIns.Name) == "toolbar" or string.lower(UIIns.Name) == "tools" then
				UIIns.Enabled = true
			end
		end
		game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
		Music.Position = Home.Position
		Exploits.Position = Home.Position
		Time.Position = UDim2.new(0.029,0,1,0)
		Info.Position = UDim2.new(0.9,0,1,0)
		local transitionInfo = TweenInfo.new(0.7, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {Rotation = 0})
		tween:Play()
		Toggle:TweenPosition(UDim2.new(0.488, 0, 0.742, 0),"Out", "Quint", 0.7)
		Functions:TweenPosition(UDim2.new(0.277, 0, 0.781, 0),"Out", "Quint", 0.7)
		wait(0.1)	
		Time:TweenPosition(UDim2.new(0.029, 0, 0.335, 0),"Out","Quint",0.6)
		wait(0.05)
		Music:TweenPosition(UDim2.new(0.4, 0, 0.2, 0),"Out","Quint",0.6)
		wait(0.15)
		Exploits:TweenPosition(UDim2.new(0.3, 0, 0.2, 0),"Out","Quint",0.6)
		wait(0.3)
		Info:TweenPosition(UDim2.new(0.9, 0, 0.2, 0),"Out","Quint",0.6)
		wait(0.7)
		Toggle.Position = UDim2.new(0.488, 0, 0.742, 0)
		uiopened = true
	end
end

function ExecuteExploit(Code)
	loadstring(game:HttpGet(Code, true))()
end

function OpenInfo()
	db = true
	Info_2.Position = UDim2.new(0.5, 0, 1.05, 0)
	Info_2.Size = UDim2.new(0.843, 0, 0.197, 0)
	if ExploitsOpen == true then
		CloseExploits()
	end
	if HomeOpen == true then
		CloseHome()
	end
	if MusicOpen == true then
		CloseMusic()
	end
	if InfoOpen == true then
		CloseInfo()
	end
	local transitionInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {ImageTransparency = 1})
	tween:Play()
	wait(0.25)
	Toggle.Visible = false
	Info_2.Visible = true
	Info_2:TweenSizeAndPosition(UDim2.new(0.956, 0 ,0.224, 0),UDim2.new(0.5, 0, 0.636, 0),"Out", "Quint", 0.59)
	wait(0.4)
	local transitionInfo = TweenInfo.new(0.75, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Info_2.Shadow, transitionInfo, {ImageTransparency = 0.75})
	tween:Play()
	wait(0.2)
	db = false
	InfoOpen = true
end
function CloseInfo()
	db = true
	Toggle.Visible = true
	local transitionInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {ImageTransparency = 0})
	tween:Play()
	Info_2:TweenSizeAndPosition(UDim2.new(0.843, 0, 0.197, 0),UDim2.new(0.5, 0, 1.05, 0),"In", "Quint", 0.59)
	wait(0.2)
	local transitionInfo = TweenInfo.new(0.75, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Info_2.Shadow, transitionInfo, {ImageTransparency = 1})
	tween:Play()
	wait(0.4)
	Info_2.Visible = false
	db = false
	InfoOpen = false
end

function OpenHome()
	db = true
	Home_2.Position = UDim2.new(0.5, 0, 1.05, 0)
	Home_2.Size = UDim2.new(0.894, 0, 0.337, 0)
	if ExploitsOpen == true then
		CloseExploits()
	end
	if HomeOpen == true then
		CloseHome()
	end
	if MusicOpen == true then
		CloseMusic()
	end
	if InfoOpen == true then
		CloseInfo()
	end
	local transitionInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {ImageTransparency = 1})
	tween:Play()
	wait(0.25)
	Toggle.Visible = false
	Home_2.Visible = true
	Home_2:TweenSizeAndPosition(UDim2.new(0.956, 0 ,0.36, 0),UDim2.new(0.5, 0, 0.569, 0),"Out", "Quint", 0.59)
	wait(0.4)
	local transitionInfo = TweenInfo.new(0.75, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Home_2.Shadow, transitionInfo, {ImageTransparency = 0.75})
	tween:Play()
	wait(0.2)
	db = false
	HomeOpen = true
end
function CloseHome()
	db = true
	Toggle.Visible = true
	local transitionInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {ImageTransparency = 0})
	tween:Play()
	
	Home_2:TweenSizeAndPosition(UDim2.new(0.894, 0, 0.337, 0),UDim2.new(0.5, 0, 1.05, 0),"In", "Quint", 0.59)
	wait(0.2)
	local transitionInfo = TweenInfo.new(0.75, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Home_2.Shadow, transitionInfo, {ImageTransparency = 1})
	tween:Play()
	wait(0.4)
	Home_2.Visible = false
	db = false
	HomeOpen = false
end

function OpenExploits()
	Exploits_2.Size = UDim2.new(0.853, 0, 0.322, 0)
	Exploits_2.Position = UDim2.new(0.5, 0, 1.05, 0)
	db = true
	if ExploitsOpen == true then
		CloseExploits()
	end
	if HomeOpen == true then
		CloseHome()
	end
	if MusicOpen == true then
		CloseMusic()
	end
	if InfoOpen == true then
		CloseInfo()
	end
	local transitionInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {ImageTransparency = 1})
	tween:Play()
	wait(0.25)
	Toggle.Visible = false
	Exploits_2.Visible = true
	Exploits_2:TweenSizeAndPosition(UDim2.new(0.956, 0, 0.36, 0),UDim2.new(0.5, 0, 0.568, 0),"Out", "Quint", 0.59)
	wait(0.4)
	local transitionInfo = TweenInfo.new(0.75, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Exploits_2.Shadow, transitionInfo, {ImageTransparency = 0.75})
	tween:Play()
	wait(0.2)
	db = false
	ExploitsOpen = true
end

function CloseExploits()
	db = true
	Toggle.Visible = true
	local transitionInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Toggle, transitionInfo, {ImageTransparency = 0})
	tween:Play()
	Exploits_2:TweenSizeAndPosition(UDim2.new(0.853, 0, 0.322, 0),UDim2.new(0.5, 0, 1.05, 0),"In", "Quint", 0.59)
	wait(0.2)
	local transitionInfo = TweenInfo.new(0.75, Enum.EasingStyle.Quint)
	local tween = game:GetService("TweenService"):Create(Exploits_2.Shadow, transitionInfo, {ImageTransparency = 1})
	tween:Play()
	wait(0.4)
	Exploits_2.Visible = false
	db = false
	ExploitsOpen = false
end

